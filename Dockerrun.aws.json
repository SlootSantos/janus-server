{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "janus",
      "host": {
        "sourcePath": "/var/app/current/janus"
      }
    },
    {
      "name": "socket",
      "host": {
        "sourcePath": "/var/run"
      }
    },
    {
      "name": "redis",
      "host": {
        "sourcePath": "/var/data/redis"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "janus",
      "image": "976589619057.dkr.ecr.us-east-1.amazonaws.com/janus/server",
      "essential": true,
      "memory": 500,
      "environment": [],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8888
        }
      ],
      "links": ["redis"],
      "mountPoints": [
        {
          "sourceVolume": "janus",
          "containerPath": "/var/janus",
          "readOnly": true
        },
        {
          "sourceVolume": "socket",
          "containerPath": "/var/run",
          "readOnly": false
        }
      ]
    },
    {
      "name": "redis",
      "image": "redis",
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "redis",
          "containerPath": "/var/data/redis",
          "readOnly": false
        }
      ]
    }
  ]
}
